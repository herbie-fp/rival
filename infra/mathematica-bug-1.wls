yre = -5464267146777197/576460752303423488
yim = 

(* Now we evaluate the function *)

N[f[xre, xim, yre, yim], 20]
(* Out[6]= 0.0011939808487326610876 *)

N[f[xre, xim, yre, yim], 30]
